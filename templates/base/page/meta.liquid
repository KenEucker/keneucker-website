<!-- META -->
<title>{{ page.meta.metaTitle }}</title>
<meta charset="utf-8" />
<meta name="viewport" content="width=device-width, initial-scale=1, user-scalable=no" />
<meta http-equiv="X-UA-Compatible" content="IE=9;IE=10;IE=Edge,chrome=1" />
<meta property="og:type" content="{{ page.meta.metaType }}" />
<meta property="og:url" content="{{ page.meta.metaUrl }}" />
<meta property="og:image" content="{{ page.meta.metaImage }}" />
<meta property="og:title" content="{{ page.meta.metaTitle }}" />
<meta property="og:description" content="{{ page.meta.metaDescription }}" />